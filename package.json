{
  "name": "medieval-store-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "ts:check": "npx tsc --noEmit",
    "prettier:check": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "stylelint:check": "stylelint src/**/*.css",
    "stylelint:fix": "stylelint src/**/*.css --fix",
    "all:check": "npx tsc --noEmit && next lint && npm run stylelint:check && npm run prettier:check",
    "all:fix": "npm run stylelint:fix && npm run prettier:fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "classnames": "^2.3.2",
    "mobx": "^6.10.2",
    "mobx-persist-store": "^1.1.3",
    "mobx-react-lite": "^4.0.5",
    "next": "^14.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-input-mask": "^2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@types/axios": "^0.14.0",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-input-mask": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^8.10.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.2.2"
  }
}
